{
  "type": "object",
  "additionalProperties": false,
  "required": ["id", "name", "urls"],
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[0-9]{13}$"
    },
    "name": {
      "type": "string"
    },
    "securities_code": {
      "type": "string",
      "pattern": "^[0-9][0-9ACDFGHJ-NPR-UWXY][0-9][0-9ACDFGHJ-NPR-UWXY]$"
    },
    "urls": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": ["url", "type"],
        "properties": {
          "type": {
            "type": "string",
            "enum": ["corporate", "service", "blog", "slide", "podcast", "job", "event", "ir"]
          },
          "url": {
            "type": "string",
            "format": "uri"
          },
          "feed": {
            "type": "string",
            "format": "uri"
          }
        }
      }
    }
  }
}
